<%= form_tag '/register', method: :post do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <%= submit_tag 'Register' %>
<% end %>

<%= link_to 'Login', "https://flat.io/auth/oauth?client_id=#{ENV['CLIENT_ID']}&response_type=code&scope=account.public_profile scores&redirect_uri=http://localhost:3000/auth/flat/callback" %>
