<%= form_for @user do |f| %>
  <%= hidden_field_tag :flat_id, @user.flat_id %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.label :about %>
  <%= f.text_field :about %>
  <%= f.label :interests, 'Genres Of Interest' %>
  <%= f.text_field :interests %>
  <%= f.label :instruments, 'Instruments I Play' %>
  <%= f.text_field :instruments %> 
  <%= f.submit 'Register' %>
<% end %>
